<wpml-config>
	<custom-fields>
		<custom-field action="translate">_shopkit_short_description</custom-field>
		<custom-field action="ignore">_shopkit_sidebar_layout</custom-field>
		<custom-field action="ignore">_shopkit_page_title</custom-field>
		<custom-field action="copy">_shopkit_post_gallery</custom-field>
		<custom-field action="copy">_shopkit_post_video_mp4</custom-field>
		<custom-field action="copy">_shopkit_post_video_ogg</custom-field>
		<custom-field action="copy">_shopkit_post_video</custom-field>
		<custom-field action="copy">_shopkit_post_audio_mpeg</custom-field>
		<custom-field action="copy">_shopkit_post_audio_ogg</custom-field>
		<custom-field action="copy">_shopkit_post_audio</custom-field>
		<custom-field action="translate">_shopkit_post_link</custom-field>
		<custom-field action="translate">_shopkit_post_quote</custom-field>
	</custom-fields>
	<admin-texts>
		<key name="shopkit_material_section_top_bar">
			<key name="top_bar_elements_on_left" />
			<key name="top_bar_elements_on_right" />
		</key>
		<key name="shopkit_material_section_header">
			<key name="header_elements_on_left" />
			<key name="header_elements_on_right" />
		</key>
		<key name="shopkit_material_section_menu_bar">
			<key name="menu_bar_elements_on_left" />
			<key name="menu_bar_elements_on_right" />
		</key>
		<key name="shopkit_material_section_footer_bar">
			<key name="footer_bar_elements_on_left" />
			<key name="footer_bar_elements_on_right" />
		</key>
	</admin-texts>
</wpml-config>